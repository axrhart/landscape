@article{ WOS:000232045000005,
Author = {Pomerantz, J},
Title = {A conceptual framework and open research questions for chat-based
   reference service},
Journal = {JOURNAL OF THE AMERICAN SOCIETY FOR INFORMATION SCIENCE AND TECHNOLOGY},
