@article{ WOS:000772683500011,
Author = {McKee, Martin and Altmann, Danny and Costello, Anthony and Friston, Karl
   and Haque, Zubaida and Khunti, Kamlesh and Michie, Susan and Oni,
   Tolullah and Pagel, Christina and Pillay, Deenan and Reicher, Steve and
