@inproceedings{ WOS:000398878100004,
Author = {Ozmen, Ozgur and Yilmaz, Levent},
Editor = {Yilmaz, L and Oren, T and Madey, G and Sierhuis, M and Zhang, Y},
Title = {An Agent-Based Information Foraging Model of Scientific Knowledge
   Creation and Spillover in Open Science Communities},
Booktitle = {AGENT-DIRECTED SIMULATION SYMPOSIUM 2012 (ADS 2012)},
Series = {Simulation Series},
Year = {2012},
Volume = {44},
Number = {1},
Pages = {24-30},
Note = {Agent-Directed Simulation Symposium (ADS 2012), Orlando, FL, MAR 26-30,
   2012},
Organization = {Soc Modeling \& Simulat},
Abstract = {Motivation and problem-domain preferences of scientists can affect
   aggregate level emergence and growth of problem domains in science. In
   this study, we introduce an agent-based model that is based on
   information foraging and expectancy theory to examine the impact of
   rationality and openness on the growth and evolution of scientific
   domains. We simulate a virtual socio-technical system, in which
   scientists with different preferences search for problem domains to
   contribute knowledge, while considering their motivational gains.
   Problem domains become mature and knowledge spills occur over time to
   facilitate creation of new problem domains. We conduct experiments to
   examine emergence and growth of clusters of domains based on local
   interactions and preferences of scientists and present preliminary
   qualitative observations.},
