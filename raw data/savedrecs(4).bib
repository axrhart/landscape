@article{ WOS:000342418200007,
Author = {Qi, Han and Shiraz, Muhammad and Liu, Jie-yao and Gani, Abdullah and
   Abdul Rahman, Zulkanain and Altameem, Torki A.},
Title = {Data center network architecture in cloud computing: review, taxonomy,
   and open research issues},
Journal = {JOURNAL OF ZHEJIANG UNIVERSITY-SCIENCE C-COMPUTERS \& ELECTRONICS},
Year = {2014},
Volume = {15},
