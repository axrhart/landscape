@article{ WOS:000600676300018,
Author = {Dworak, Elizabeth M. and Revelle, William and Doebler, Philip and
   Condon, David M.},
Title = {Using the International Cognitive Ability Resource as an open source
   tool to explore individual differences in cognitive ability},
Journal = {PERSONALITY AND INDIVIDUAL DIFFERENCES},
Year = {2021},
Volume = {169},
Number = {SI},
Month = {FEB 1},
Abstract = {Although the measurement of intelligence is important, researchers
   sometimes avoid using them in their studies due to their history, cost,
   or burden on the researcher. To encourage the use of cognitive ability
   items in research, we discuss the development and validation of the
   International Cognitive Ability Resource (ICAR), a growing set of items
   from 19 different subdomains. We consider how these items might benefit
   open science in contrast to more established proprietary measures. A
   short summary of how these items have been used in outside studies is
   provided in addition to ways we would love to see the use of
   public-domain cognitive ability items grow.},
