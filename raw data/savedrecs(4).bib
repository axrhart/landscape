@article{ WOS:000715286800001,
Author = {Nigro, Cosimo and Hassan, Tarek and Olivera-Nieto, Laura},
Title = {Evolution of Data Formats in Very-High-Energy Gamma-Ray Astronomy},
Journal = {UNIVERSE},
