@article{ WOS:000491207200001,
Author = {Pauliuk, Stefan and Heeren, Niko},
Title = {ODYM-An open software framework for studying dynamic material systems:
   Principles, implementation, and data structures},
Journal = {JOURNAL OF INDUSTRIAL ECOLOGY},
Year = {2020},
Volume = {24},
