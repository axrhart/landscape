@article{ WOS:000488651700013,
Author = {Gilroy, Shawn P. and Kaplan, Brent A.},
Title = {Furthering Open Science in Behavior Analysis: An Introduction and
   Tutorial for Using GitHub in Research},
Journal = {PERSPECTIVES ON BEHAVIOR SCIENCE},
