@article{ WOS:000488924100002,
Author = {Pauliuk, Stefan and Heeren, Niko and Hasan, Mohammad Mahadi and Mueller,
   Daniel B.},
Title = {A general data model for socioeconomic metabolism and its implementation
   in an industrial ecology data commons prototype},
Journal = {JOURNAL OF INDUSTRIAL ECOLOGY},
