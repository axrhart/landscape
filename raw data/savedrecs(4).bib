@article{ WOS:000616763400014,
Author = {Anderson, Kristin R. and Harris, Julie A. and Ng, Lydia and Prins, Pjotr
   and Memar, Sara and Ljungquist, Bengt and Furth, Daniel and Williams,
   Robert W. and Ascoli, Giorgio A. and Dumitriu, Dani},
Title = {Highlights from the Era of Open Source Web-Based Tools},
Journal = {JOURNAL OF NEUROSCIENCE},
Year = {2021},
Volume = {41},
Number = {5},
Pages = {927-936},
Month = {FEB 3},
Abstract = {High digital connectivity and a focus on reproducibility are
   contributing to an open science revolution in neuroscience. Repositories
   and platforms have emerged across the whole spectrum of subdisciplines,
   paving the way for a paradigm shift in the way we share, analyze, and
   reuse vast amounts of data collected across many laboratories. Here, we
   describe how open access web-based tools are changing the landscape and
