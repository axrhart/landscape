Title = {NanoAsPipe: A transcriptome analysis and alternative splicing detection
   pipeline for MinION long-read RNA-seq},
Booktitle = {2017 IEEE INTERNATIONAL CONFERENCE ON BIOINFORMATICS AND BIOMEDICINE
   (BIBM)},
