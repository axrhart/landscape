@article{ WOS:000475287900011,
Author = {Duh, Emilija Stojmenova and Duh, Andrej and Droftina, Uros and Kos, Tim
   and Duh, Urban and Korosak, Tanja Simonic and Korosak, Dean},
Title = {Publish-and-Flourish: Using Blockchain Platform to Enable Cooperative
   Scholarly Communication},
Journal = {PUBLICATIONS},
Year = {2019},
Volume = {7},
Number = {2},
Month = {JUN},
Abstract = {Scholarly communication is today immersed in publish-or-perish culture
   that propels non-cooperative behavior in the sense of strategic games
   played by researchers. Here we introduce and describe a blockchain based
   platform for decentralized scholarly communication. The design of the
   platform rests on community driven publishing reviewing processes and
   implements cryptoeconomic incentives that promote cooperative user
   behavior. The key to achieve cooperation in blockchain based scholarly
   communication is to transform today's static research paper into a
   modifiable research paper under continuous peer review process. We
   introduce and discuss the implementation of a modifiable research paper
   as a smart contract on the blockchain.},
