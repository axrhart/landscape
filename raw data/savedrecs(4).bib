@article{ WOS:000541150100019,
Author = {Saad, Walid and Bennis, Mehdi and Chen, Mingzhe},
Title = {A Vision of 6G Wireless Systems: Applications, Trends, Technologies, and
   Open Research Problems},
Journal = {IEEE NETWORK},
Year = {2020},
