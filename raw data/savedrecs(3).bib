@article{ WOS:000471782800003,
Author = {Olesk, Arko and Kaal, Esta and Toom, Kristel},
Title = {The possibilities of Open Science for knowledge transfer in the
   science-policy interface},
Journal = {JCOM-JOURNAL OF SCIENCE COMMUNICATION},
Year = {2019},
Volume = {18},
Number = {3, SI},
Abstract = {This paper explores the possible role of Open Science in the knowledge
   transfer between research and policy, focusing on its potential use by
   scientific councillors at Estonian ministries. Qualitative interviews
   with scientific councillors show that they perceive their role as
   intermediaries between research and policy and focus their work on
   improving the quality of research commissioned by their ministry. This
   process, for them, involves using existing academic articles and
   datasets to which, however, they lack official access. We show that Open
   Science can contribute to knowledge transfer if there are knowledge
   brokers in public sector organizations.},
