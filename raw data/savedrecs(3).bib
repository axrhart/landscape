@article{ WOS:000621235900003,
Author = {Choi, Ye Jin and Choi, Hyung Wook and Kim, Soon},
Title = {Preprint acceptance policies of Asian academic society journals in 2020},
Journal = {SCIENCE EDITING},
Year = {2021},
Volume = {8},
Number = {1},
Pages = {10-17},
Month = {FEB},
Abstract = {Purpose: In the current era of the coronavirus disease 2019 pandemic,
   the trend of sharing new research results through preprint platforms is
   receiving more attention from researchers than ever before. Preprints
   have been recognized as a primary and essential method to disseminate
   new findings faster than traditional publications. Therefore, it has
   become necessary for journals and editors to acknowledge these changes,
   prepare preprint policies, and notify authors accordingly. This study
   aimed to review the status of preprint policies of international
   publishers and Asian academic society journals.
   Methods: In total, 383 Asian academic society journals registered in
   Science Citation Index Expanded were selected as a dataset for analysis
   between December 11, 2020 and January 8, 2021. Three different
   parameters were investigated whether each journal had a preprint policy,
   whether journals allowed preprint manuscripts to be submitted, and
   whether preprint articles were allowed to be included in the references.
   Results: Among the 383 Asian academic society journals from 22
   countries, only 28 journals accepted preprint manuscripts, and eight
   allowed the use of preprint manuscripts as references. Japan had the
   most journals that both had preprint policies and accepted preprint
   manuscripts, with 13 journals, followed by Korea with 10 journals.
   Conclusion: Despite the limitations of this study, the results show that
   editors and journal staff should understand the current preprint trend
   and try to prepare preprint policies that best meet the journals' and
   authors' interests.},
