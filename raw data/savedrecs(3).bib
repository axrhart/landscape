@inproceedings{ WOS:000772157700011,
Author = {Lee, Jian-Sin},
Editor = {Smits, M},
Title = {Setting up a Checkpoint for Research on the Prevalence of Journal Data
   Policies: A Systematic Review},
Booktitle = {INFORMATION FOR A BETTER WORLD: SHAPING THE GLOBAL FUTURE, PT I},
Series = {Lecture Notes in Computer Science},
