@article{ WOS:000544040100001,
Author = {Coello Coello, Carlos A. and Gonzalez Brambila, Silvia and Figueroa
   Gamboa, Josue and Castillo Tapia, Ma Guadalupe and Hernandez Gomez,
   Raquel},
Title = {Evolutionary multiobjective optimization: open research areas and some
   challenges lying ahead},
Journal = {COMPLEX \& INTELLIGENT SYSTEMS},
Year = {2020},
Volume = {6},
Number = {2},
Pages = {221-236},
Month = {JUL},
Abstract = {Evolutionary multiobjective optimization has been a research area since
   the mid-1980s, and has experienced a very significant activity in the
   last 20 years. However, and in spite of the maturity of this field,
   there are still several important challenges lying ahead. This paper
