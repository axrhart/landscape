Title = {Quality Guidelines for Research Artifacts in Model-Driven Engineering},
Booktitle = {24TH INTERNATIONAL CONFERENCE ON MODEL-DRIVEN ENGINEERING LANGUAGES AND
   SYSTEMS (MODELS 2021)},
Year = {2021},
Pages = {285-296},
