@article{ WOS:000468761400009,
Author = {Branney, Peter and Reid, Kate and Frost, Nollaig and Coan, Susan and
   Mathieson, Amy and Woolhouse, Maxine},
Title = {A context-consent meta-framework for designing open (qualitative) data
   studies},
Journal = {QUALITATIVE RESEARCH IN PSYCHOLOGY},
Year = {2019},
Volume = {16},
Number = {3, SI},
Pages = {483-502},
Month = {JUL 3},
Abstract = {To date, open science, particularly open data, in psychology has focused
   on quantitative research. This article aims to explore ethical and
   practical issues encountered by UK-based psychologists utilising open
   qualitative datasets. Semi-structured telephone interviews with eight
   qualitative psychologists were explored using a framework analysis. From
   the findings, we offer a context-consent meta-framework as a resource to
   help in the design of studies sharing their data and/or studies using
   open data. We recommend secondary studies conduct archaeologies of
   context and consent to examine if the data available are suitable for
   their research questions. This research is the first we know of in the
   study of ``doing{''} (or not doing) open science, which could be
   repeated to develop a longitudinal picture or complemented with
   additional approaches, such as observational studies of how context and
   consent are negotiated in preregistered studies and open data.},
