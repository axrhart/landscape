@article{ WOS:000708950800001,
Author = {Kirtley, Olivia J. and Lafit, Ginette and Achterhof, Robin and
   Hiekkaranta, Anu P. and Myin-Germeys, Inez},
Title = {Making the Black Box Transparent: A Template and Tutorial for
   Registration of Studies Using Experience-Sampling Methods},
