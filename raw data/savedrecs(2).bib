@inproceedings{ WOS:000524679600038,
Author = {Kanterakis, Alexandros and Iatraki, Galateia and Pityanou, Konstantina
   and Koumakis, Lefteris and Kanakaris, Nikos and Karacapilidis, Nikos and
   Potamias, George},
