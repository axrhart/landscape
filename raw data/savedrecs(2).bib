@article{ WOS:000639720600001,
Author = {Balazs, Peter and Kasess, Christian and Kreuzer, Wolfgang and Maly,
   Thomas and Prusa, Zdenek and Jaillet, Florent},
Title = {Application of frame multipliers for the extraction of curve squeals
   from train signals},
Journal = {ELEKTROTECHNIK UND INFORMATIONSTECHNIK},
Year = {2021},
Volume = {138},
Number = {3},
Pages = {206-211},
Month = {JUN},
Abstract = {For many applications in acoustics it is necessary to process signals
   and functions with the help of time-variant filters, e.g., to remove
   components from a signal whose frequency profile changes over time. A
   method is presented that is based on a representation of the signal in
   terms of frames, where such time-varying filters can be defined in the
   time frequency plane. After a brief description of the theoretical
   background of frames, their application is explained using an example
   from noise research. With the help of a simple graphical user interface,
   a component (curve squeal) that is not time-invariant due to the Doppler
   effect is cut out of a recording of a train and inserted into a second
   signal. This way, controlled signals can be generated, which can then be
   used for noise assessment.},
