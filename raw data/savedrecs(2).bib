@article{ WOS:000624345700001,
Author = {Riley, Ekemini Au and Schekman, Randy},
Title = {Open science takes on Parkinson's disease},
Journal = {ELIFE},
Year = {2021},
Volume = {10},
Month = {FEB 25},
Abstract = {The Aligning Science Across Parkinson's (ASAP) initiative was set up to
   improve understanding of the biology underlying the onset and
   progression of Parkinson's disease. With an emphasis on open science and
   collaboration, we have assembled a research network led by nearly 100
   investigators to explore the pathology of Parkinson's disease, and this
   network will soon expand to include researchers working on relevant
   (dys)-functional neural circuits. We have also contributed to
   large-scale genetics and patient cohort initiatives related to the
   disease. We hope that these actions, and others planned for the future,
   will deepen our knowledge of the molecular mechanisms underlying the
   origin and evolution of Parkinson's disease and, ultimately, contribute
   to the development of novel therapies.},
