@article{ WOS:000478024300007,
Author = {Lynam, Donald R. and Hyatt, Courtland S. and Hopwood, Christopher J. and
   Wright, Aidan G. C. and Miller, Joshua D.},
Title = {Should Psychologists Sign Their Reviews? Some Thoughts and Some Data},
Journal = {JOURNAL OF ABNORMAL PSYCHOLOGY},
Year = {2019},
Volume = {128},
Number = {6, SI},
Pages = {541-546},
Month = {AUG},
Abstract = {The Open Science Movement (OSM) emphasizes increased transparency at
   many of the steps in the scientific process and has improved
   psychological science. In the present article, we discuss whether such
   transparency should find its way into the review process. We discuss a
   priori thoughts and intuitions about the costs and benefits of signing
   reviews. In terms of benefits, these include greater alignment with OSM
   and greater accountability leading to increases in civility, care, and
   thoughtfulness of reviews. The most obvious cost is potential
   retaliation for negative reviews. To check these intuitions, we surveyed
   a sample of 358 faculty members about their experience and views on
   signing reviews. Results both underscored and extended the initial
   intuitions. Results suggest there are many benefits to increasing the
   incidence of reviewers signing their reviews. Fears of retaliation seem
   to be somewhat exaggerated. We discuss possible means of reducing the
   possibility of retaliation.},
