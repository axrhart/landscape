@article{ WOS:000539769600001,
Author = {Gabrielsen, Ane Moller},
Title = {Openness and trust in data-intensive science: the case of biocuration},
Journal = {MEDICINE HEALTH CARE AND PHILOSOPHY},
Year = {2020},
Volume = {23},
Number = {3},
Pages = {497-504},
Month = {SEP},
Abstract = {Data-intensive science comes with increased risks concerning quality and
   reliability of data, and while trust in science has traditionally been
   framed as a matter of scientists being expected to adhere to certain
   technical and moral norms for behaviour, emerging discourses of open
   science present openness and transparency as substitutes for established
   trust mechanisms. By ensuring access to all available information,
   quality becomes a matter of informed judgement by the users, and trust
   no longer seems necessary. This strategy does not, however, take into
   consideration the networks of professionals already enabling
   data-intensive science by providing high-quality data. In the life
   sciences, biological data- and knowledge bases managed by expert
   biocurators have become crucial for data-intensive research. In this
   paper, I will use the case of biocurators to argue that openness and
   transparency will not diminish the need for trust in data-intensive
   science. On the contrary, data-intensive science requires a
   reconfiguration of existing trust mechanisms in order to include those
   who take care of and manage scientific data after its production.},
