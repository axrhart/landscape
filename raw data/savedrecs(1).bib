@article{ WOS:000659162100001,
Author = {Cox, Andrew and Abbott, Pamela},
Title = {Librarians' Perceptions of the Challenges for Researchers in Rwanda and
   the Potential of Open Scholarship},
Journal = {LIBRI-INTERNATIONAL JOURNAL OF LIBRARIES AND INFORMATION STUDIES},
