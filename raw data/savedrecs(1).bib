@article{ WOS:000683352400001,
Author = {Hocker, Julian and Bipat, Taryn and McDonald, David W. and Zachry, Mark},
Title = {Evaluating QualiCO: an ontology to facilitate qualitative methods
   sharing to support open science},
Journal = {JOURNAL OF INTERNET SERVICES AND APPLICATIONS},
Year = {2021},
Volume = {12},
Number = {1},
Month = {DEC},
Abstract = {Qualitative science methods have largely been omitted from discussions
   of open science. Platforms focused on qualitative science that support
   open science data and method sharing are rare. Sharing and exchanging
   coding schemas has great potential for supporting traceability in
   qualitative research as well as for facilitating the reuse of coding
   schemas. In this study, we present and evaluate QualiCO, an ontology to
   describe qualitative coding schemas. Twenty qualitative researchers used
   QualiCO to complete two coding tasks. In our findings, we present task
   performance and interview data that focus participants' attention on the
   ontology. Participants used QualiCO to complete the coding tasks,
   decreasing time on task, while improving accuracy, signifying that
   QualiCO enabled the reuse of qualitative coding schemas. Our discussion
   elaborates some issues that participants had and highlights how
   conceptual and prior practice frames their interpretation of how QualiCO
   can be used.},
