@article{ WOS:000454366100004,
Author = {Peset, Fernanda and Ferrer-Sapena, Antonia and Garzon-Farinos, Fernanda},
Title = {Documentation in Otolaryngology. Sharing Otolaryngology research data in
   an open science ecosystem},
Journal = {REVISTA ORL},
Year = {2018},
Volume = {9},
Number = {4},
Pages = {265-271},
Abstract = {Introduction and objective: The present text addresses the most
   significant aspects to share research data in otolaryngology in the
   context of open science as an ecosystem. Its aim is to offer a panoramic
   view that helps the researcher to manage their data as part of enriched
   science. Method: A bibliographical review and of the own experience in
   the field of the investigation data was performed. Results: The basic
   pillars for success are offered: its political, technical and necessary
   capacities. Discussion: The tasks of making data available should be
   recognized as part of the researcher's curriculum because documenting
   them to be reusable is a highly specialized and time-consuming task.
   Conclusions: It is considered that we are at a crucial moment to begin
   to share data. It is being considered in all scientific policy scenarios
   as in the EU through the European Open Science Computing.},
