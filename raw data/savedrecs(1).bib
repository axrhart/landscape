@inproceedings{ WOS:000561058500015,
Author = {Biesenbender, Sophie and Petersohn, Sabrina and Thiedig, Christoph},
Editor = {Sicilia, MA and Simons, E and Clements, A and DeCastro, P and Bergstrom, J},
Title = {Using Current Research Information Systems (CRIS) to showcase national
   and institutional research (potential): research information systems in
   the context of Open Science},
Booktitle = {14TH INTERNATIONAL CONFERENCE ON CURRENT RESEARCH INFORMATION SYSTEMS
   (CRIS2018): FAIRNESS OF RESEARCH INFORMATION},
Series = {Procedia Computer Science},
Year = {2019},
Volume = {146},
Pages = {142-155},
Note = {14th International Conference on Current Research Information Systems
   (CRIS) - FAIRness of Research Information, Univ Umea, Umea, SWEDEN, JUN,
   2018},
Abstract = {This paper analyzes the factors that contribute or hinder the (joint)
   adoption of current research information systems (CRIS) and Open Access
   repositories (OAR) as manifestations of the Open Science principle.
   Given the significance attached to Open Science principles such as
   transparency and accessibility, we ask whether CRIS are able to reflect
   these priorities by providing technical interfaces or integrating OAR
   into the CRIS structure. We suggest that the interplay of national and
   institutional governance structures and policies is crucial for the
   adequate and efficient integration of CRIS and Open Science. To account
   for variances in both national and institutional context factors, we
   compare research institutions in three countries: Italy, the Netherlands
   and Germany. The qualitative exploratory analysis suggests that the
