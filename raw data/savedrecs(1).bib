@article{ WOS:000437167100006,
Author = {Lopes, Carlos and Antunes, Maria da Luz and Sanches, Tatiana},
Title = {The contribution of information literacy to Open Science},
Journal = {IBERSID-REVISTA DE SISTEMAS DE INFORMACION Y DOCUMENTACION},
Year = {2018},
Volume = {12},
Number = {1},
Pages = {59-67},
Month = {JAN-JUN},
Abstract = {The scope of Open Science translates into a collaborative, transparent
   process of dissemination, creation, and transfer of knowledge,
   accessible to research and based on the principles of open access. The
   different agents of the research process, equipped with a set of
   information literacy skills, acquire the ability, in print or digital
   information environments, and based on their own critical and reflexive
   thinking, to transform information into new knowledge. This article
   explores the integration of Open Science concepts in information
   literacy. It presents a theoretical reflection that shows the
   contributions of information literacy in the academic context and in the
   dynamics of the production of science. It is concluded that information
   literacy is an essential learning tool for the development of Open
   Science, enhancing the critical understanding of content, as well as the
   development and progress of research.},
