@article{ WOS:000576664200027,
Author = {Murphy, Mary C. and Mejia, Amanda F. and Mejia, Jorge and Yan, Xiaoran
   and Cheryan, Sapna and Dasgupta, Nilanjana and Destin, Mesmin and
   Fryberg, Stephanie A. and Garcia, Julie A. and Haines, Elizabeth L. and
   Harackiewicz, Judith M. and Ledgerwood, Alison and Moss-Racusin, Corinne
   A. and Park, Lora E. and Perry, Sylvia P. and Ratliff, Kate A. and
   Rattan, Aneeta and Sanchez, Diana T. and Savani, Krishna and
   Sekaquaptewa, Denise and Smith, Jessi L. and Taylor, Valerie Jones and
   Thoman, Dustin B. and Wout, Daryl A. and Mabry, Patricia L. and Ressl,
   Susanne and Diekman, Amanda B. and Pestilli, Franco},
Title = {Open science, communal culture, and women's participation in the
   movement to improve science},
Journal = {PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF
   AMERICA},
Year = {2020},
Volume = {117},
Number = {39},
Pages = {24154-24164},
Month = {SEP 29},
Abstract = {Science is undergoing rapid change with the movement to improve science
   focused largely on reproducibility/replicability and open science
   practices. This moment of change-in which science turns inward to
   examine its methods and practices-provides an opportunity to address its
   historic lack of diversity and noninclusive culture. Through network
   modeling and semantic analysis, we provide an initial exploration of the
   structure, cultural frames, and women's participation in the open
   science and reproducibility literatures (n = 2,926 articles and
   conference proceedings). Network analyses suggest that the open science
   and reproducibility literatures are emerging relatively independently of
   each other, sharing few common papers or authors. We next examine
   whether the literatures differentially incorporate collaborative,
   prosocial ideals that are known to engage members of underrepresented
   groups more than independent, winner-takes-all approaches. We find that
   open science has a more connected, collaborative structure than does
   reproducibility. Semantic analyses of paper abstracts reveal that these
   literatures have adopted different cultural frames: open science
   includes more explicitly communal and prosocial language than does
   reproducibility. Finally, consistent with literature suggesting the
   diversity benefits of communal and prosocial purposes, we find that
   women publish more frequently in high-status author positions (first or
   last) within open science (vs. reproducibility). Furthermore, this
   finding is further patterned by team size and time. Women are more
   represented in larger teams within reproducibility, and women's
   participation is increasing in open science over time and decreasing in
   reproducibility. We conclude with actionable suggestions for cultivating
   a more prosocial and diverse culture of science.},
