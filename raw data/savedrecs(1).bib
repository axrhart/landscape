@article{ WOS:000366854800007,
Author = {Grand, Ann and Wilkinson, Clare and Bultitude, Karen and Winfield, Alan
   F. T.},
Title = {Mapping the hinterland: Data issues in open science},
Journal = {PUBLIC UNDERSTANDING OF SCIENCE},
Year = {2016},
Volume = {25},
Number = {1},
Pages = {88-103},
Month = {JAN},
Abstract = {Open science is a practice in which the scientific process is shared
   completely and in real time. It offers the potential to support
   information flow, collaboration and dialogue among professional and
   non-professional participants. Using semi-structured interviews and case
   studies, this research investigated the relationship between open
   science and public engagement. This article concentrates on three
   particular areas of concern that emerged: first, how to effectively
   contextualise and narrate information to render it accessible, as
   opposed to simply available; second, concerns about data quantity and
   quality; and third, concerns about the skills required for effective
   contextualisation, mapping and interpretation of information.},
