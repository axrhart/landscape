@article{ WOS:000744551700001,
Author = {Shaw, Adrienne and Scharkow, Michael and Wang, Zheng Joyce},
Title = {Opening a Conversation on Open Communication Research},
Journal = {JOURNAL OF COMMUNICATION},
Year = {2021},
Volume = {71},
Number = {5},
Pages = {677-685},
Month = {OCT},
Abstract = {Many disciplines have been debating and enacting a range of policies,
   procedures, and practices that fall under the umbrella term ``open
   research{''} or ``open science.{''} Following the publication of ``An
   Agenda for Open Science in Communication{''}, we invited communication
   scholars to continue the conversation on what open research practices
   broadly might mean for our diverse field. Specifically, we sought work
   that: looked empirically at the need for and impact of open research
   practices; considered the unintended consequences of calls for open
   research practices broadly; and that reflected on what such a move would
   mean for qualitative and humanistic communication research. We hope the
   collection of articles in this special issue motivates and facilitates
   an ongoing conversation on open research practices in the field of
   communication.},
Publisher = {OXFORD UNIV PRESS INC},
Address = {JOURNALS DEPT, 2001 EVANS RD, CARY, NC 27513 USA},
Type = {Editorial Material},
Language = {English},
