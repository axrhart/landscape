@inproceedings{ WOS:000434481000024,
Author = {Bardi, Alessia and Casarosa, Vittore and Manghi, Paolo},
Editor = {Serra, G and Tasso, C},
Title = {The European Project OpenUP: OPENing UP New Methods, Indicators and
   Tools for Peer Review, Impact Measurement and Dissemination of Research
   Results},
Booktitle = {DIGITAL LIBRARIES AND MULTIMEDIA ARCHIVES, IRCDL 2018},
