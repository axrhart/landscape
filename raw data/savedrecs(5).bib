@inproceedings{ WOS:000551093101004,
Author = {Todorova, Tania and Garvanova, Magdalena and Peteva, Mariana and
   Avramova, Vanya},
Editor = {Chova, LG and Martinez, AL and Torres, IC},
Title = {COMPARATIVE FINDINGS FROM DATA LITERACY SURVEY IN THREE BULGARIAN
   UNIVERSITIES},
Booktitle = {EDULEARN19: 11TH INTERNATIONAL CONFERENCE ON EDUCATION AND NEW LEARNING
   TECHNOLOGIES},
Series = {EDULEARN Proceedings},
Year = {2019},
Pages = {932-940},
Note = {11th International Conference on Education and New Learning Technologies
   (EDULEARN), Palma, SPAIN, JUL 01-03, 2019},
Abstract = {In 2016 the international research group for implementation of the
   scientific survey on DL (Data Literacy) and RDM (Research Data
   Management) were established {[}1]. Convinced that the RDMS (Research
   Data Management Survey) was both timely and necessary, a team of
   researchers from the ULSIT (University of Library Studies and
   Information Technologies) joined the international scientific group and
   conducted the research in 2017 {[}2]. In the period of March-May 2018,
   the RDMS was replicated in UCTM (University of Chemical Technology and
