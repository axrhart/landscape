@article{ WOS:000450055400001,
Author = {Blaylock, Brian K. and Horel, John D. and Galli, Chris},
Title = {High-Resolution Rapid Refresh Model Data Analytics Derived on the Open
   Science Grid to Assist Wildland Fire Weather Assessment},
Journal = {JOURNAL OF ATMOSPHERIC AND OCEANIC TECHNOLOGY},
Year = {2018},
Volume = {35},
