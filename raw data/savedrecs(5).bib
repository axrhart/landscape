@article{ WOS:000464059400001,
Author = {Sadeghi, Afshin and Capadisli, Sarven and Wilm, Johannes and Lange,
   Christoph and Mayr, Philipp},
Title = {Opening and Reusing Transparent Peer Reviews with Automatic Article
   Annotation},
