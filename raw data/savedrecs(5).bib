@article{ WOS:000408253800005,
Author = {Verma, Shikhar and Kawamoto, Yuichi and Fadlullah, Zubair Md. and
   Nishiyama, Hiroki and Kato, Nei},
Title = {A Survey on Network Methodologies for Real-Time Analytics of Massive IoT
   Data and Open Research Issues},
Journal = {IEEE COMMUNICATIONS SURVEYS AND TUTORIALS},
Year = {2017},
Volume = {19},
Number = {3},
Pages = {1457-1477},
Abstract = {With the widespread adoption of the Internet of Things (IoT), the number
   of connected devices is growing at an exponential rate, which is
   contributing to ever-increasing, massive data volumes. Real-time
   analytics on the massive IoT data, referred to as the ``real-time IoT
   analytics{''} in this paper, is becoming the mainstream with an aim to
   provide an immediate or non-immediate actionable insights and business
   intelligence. However, the analytics network of the existing IoT systems
   does not adequately consider the requirements of the real-time IoT
   analytics. In fact, most researchers overlooked an appropriate design of
   the IoT analytics network while focusing much on the sensing and
   delivery networks of the IoT system. Since much of the IoT analytics
   network has often been taken as granted, the survey, in this paper, we
   aim to review the state-of-the-art of the analytics network
   methodologies, which are suitable for real-time IoT analytics. In this
   vein, we first describe the basics of the real-time IoT analytics, use
   cases, and software platforms, and then explain the shortcomings of the
   network methodologies to support them. To address those shortcomings, we
   then discuss the relevant network methodologies which may support the
   real-time IoT analytics. Also, we present a number of prospective
   research problems and future research directions focusing on the network
   methodologies for the real-time IoT analytics.},
Publisher = {IEEE-INST ELECTRICAL ELECTRONICS ENGINEERS INC},
Address = {445 HOES LANE, PISCATAWAY, NJ 08855-4141 USA},
Type = {Article},
Language = {English},
Affiliation = {Verma, S (Corresponding Author), Tohoku Univ, Grad Sch Informat Sci, Sendai, Miyagi 9808579, Japan.
   Verma, Shikhar; Kawamoto, Yuichi; Fadlullah, Zubair Md.; Nishiyama, Hiroki; Kato, Nei, Tohoku Univ, Grad Sch Informat Sci, Sendai, Miyagi 9808579, Japan.},
