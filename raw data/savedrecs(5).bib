@article{ WOS:000513137800006,
Author = {Robinson, Erick and Nicholson, Christopher and Kelly, Robert L.},
Title = {The Importance of Spatial Data to Open-Access National Archaeological
   Databases and the Development of Paleodemography Research},
Journal = {ADVANCES IN ARCHAEOLOGICAL PRACTICE},
