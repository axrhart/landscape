@article{ WOS:000379511200015,
Author = {Khan, Zaheer and Vasilakos, Athanasios V. and Barua, Bidushi and
   Shahabuddin, Shahriar and Ahmadi, Hamed},
Title = {Cooperative content delivery exploiting multiple wireless interfaces:
   methods, new technological developments, open research issues and a case
   study},
