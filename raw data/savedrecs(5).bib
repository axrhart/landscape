@inproceedings{ WOS:000315360600045,
Author = {Stodden, Victoria and Hurlin, Christophe and Perignon, Christophe},
Book-Group-Author = {IEEE},
Title = {RunMyCode.org: a novel dissemination and collaboration platform for
   executing published computational results},
